    <script>
        $(".ar-input").on("keypress", function (event) {
            var englishAlphabet = /^[A-Za-z]*$/;
            //var arabicAlphabet = /[\u0600-\u06ff]|[\u0621-\u064A0-9 ]|[\u0750-\u077f]|[\ufb50-\ufc3f]|[\ufe70-\ufefc]|[\u0200]|[\u00A0]/g;
            var key = String.fromCharCode(event.which);
            // For the keyCodes, look here: http://stackoverflow.com/a/3781360/114029
            // keyCode == 8  is backspace
            // keyCode == 32  is space
            // keyCode == 37 is left arrow
            // keyCode == 39 is right arrow
            // if (event.keyCode == 8 || event.keyCode == 32 | event.keyCode == 37 || event.keyCode == 39 || arabicAlphabet.test(key)) {
            if (!englishAlphabet.test(key)) {
                return true;
            } return false;
        });

        $('.ar-input').on("paste", function (e) {
